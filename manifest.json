{
    "manifest_version": 3,
    "name": "Youtube Ads Skip Ext",
    "description": "Ads Skip",
    "version": "1.0",
    "content_scripts": [{
        "js": ["script.js"],
        "run_at": "document_idle",
        "matches": ["<all_urls>"]
    }],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}
